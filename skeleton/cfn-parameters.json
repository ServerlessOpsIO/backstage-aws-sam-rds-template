{
    "Domain": "${{ values.domain_name }}",
    "System": "${{ values.system_name }}",
    "Component": $env.GITHUB_REPOSITORY_NAME_PART_SLUG_CS,
    "CodeBranch": $env.GITHUB_REF_SLUG_CS,
    "VpcId": "/org/networking/VpcId",
    "VpcSubnets": "/org/networking/VpcPrivateSubnets",
    "DbEngine": "${{ values.db_engine}}",
    "DbEngineVersion": "${{ values.db_engine_version }}",
    "DbFamily": "${{ values.db_engine}}${{ values.db_engine_version }}",
    "DbPort": "${{ values.db_port }}",
    "DbUsername": "${{ values.db_username }}",
{% if values.db_name -%}    "DbName": "${{ values.db_name }}",{% endif -%}
    "RdsScalingMinCapacity": "${{ values.min_capacity }}",
    "RdsScalingMaxCapacity": "${{ values.max_capacity }}",
    "RdsScalingMinInstances": "${{ values.min_instances }}",
    "RdsScalingMaxInstances": "${{ values.max_instances }}",
    "RdsBackupRetentionPeriod": "7",
    "RdsPreferredMaintenanceWindow": "sun:1:00-sun:03:00",
    "RdsPreferredBackupWindow": "03:01-05:00"
}